import java.util.*; 
class num1 
{ 
public static void main(String args[]) 
{ 
Scanner sc=new Scanner(System.in); 
int n; 
int r,even=0,odd=0;
 n=sc.nextInt(); 
while(n!=0) 
 { 
r = n%10;
 n= n/10; 
if( r %2 ==0) 
 { 
even= (even*10)+r; 
} 
else 
{ 
odd = (odd*10)+r; 
} 
}
 System.out.println(even+" "+odd);
 } 
}
